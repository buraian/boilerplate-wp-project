{
  "name": "brianclark/debaser-project",
  "description": "A developer-centric project setup for WordPress.",
  "authors": [
    {
      "name": "Brian Clark",
      "email": "br.clark.77@gmail.com"
    }
  ],
  "require": {
    "composer/installers": "v1.0.23",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "v1.1.1",
    "wordpress/wordpress": "4.4.2",
    "wpackagist-plugin/fast-page-switch": "1.3.1",
    "wpackagist-plugin/google-analytics-for-wordpress": "5.4.6",
    "wpackagist-plugin/groups": "1.10.1",
    "wpackagist-plugin/members": "1.1.1",
    "wpackagist-plugin/siteorigin-panels": "2.4.2",
    "wpackagist-plugin/tinymce-advanced": "4.2.8",
    "wpackagist-plugin/user-switching": "1.0.9",
    "wpackagist-plugin/wordpress-seo": "3.1.2",
    "wpackagist-plugin/wp-fail2ban": "2.3.2"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "4.4.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.4.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "webroot-dir": "wordpress",
    "webroot-package": "wordpress/wordpress",
    "installer-paths": {
      "build/plugins/{$name}/": ["type:wordpress-plugin"],
      "build/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
